.deliver .time {
  display: flex;
  padding: 10px 10px 5px;
}
.deliver .time ._span {
  font-size: 22px;
}
.deliver .time .time_l {
  width: 50%;
  text-align: center;
}
.deliver .time .time_m {
  border-left: 1px solid #F2F2F2;
  margin-top: 10px;
  height: 20px;
}
.deliver .buzhou {
  padding: 10px 10px 1px;
}
.deliver .buzhou .bz {
  display: flex;
  justify-content: space-between;
}
.deliver .buzhou .bz .bz_l {
  width: 50%;
}
.deliver .buzhou .bz .bz_l .bz_l_01 {
  font-weight: 600;
}
.deliver .buzhou .bz .bz_l .bz_l_02 {
  font-size: 12px;
}
.deliver .buzhou .bz .cu-btn {
  width: 100px;
}
.deliver .buzhou .pic {
  height: 80px;
}
.deliver .mingxi {
  padding: 10px;
}
.deliver .mingxi .mx {
  display: flex;
  padding-bottom: 10px;
}
.deliver .mingxi .mx .mx_l {
  width: 80px;
  flex-shrink: 0;
}
.deliver .mingxi .mx .mx_r {
  color: #767676;
}
.deliver .bbtn {
  display: flex;
  justify-content: space-between;
  position: fixed;
  bottom: 0;
  width: 100%;
}
.deliver .bbtn .bbtn_10 {
  width: 100%;
}

