.ad {
  font-size: 12px;
  color: #cccccc;
  text-align: center;
  margin-top: 10px;
}
.navigation .map {
  height: calc(100vh - 250px);
  width: 100%;
}
.navigation .map image {
  width: 100%;
  height: 300px;
}
.navigation .mingxi {
  padding: 10px;
}
.navigation .mingxi .mx {
  display: flex;
  padding-bottom: 10px;
}
.navigation .mingxi .mx .mx_l {
  width: 90px;
  flex-shrink: 0;
  text-align: right;
  padding-right: 20px;
}
.navigation .mingxi .mx .mx_r {
  color: #767676;
}
.navigation .mingxi .mxs {
  display: flex;
  padding-bottom: 10px;
}
.navigation .mingxi .mxs .mxs_l {
  width: 90px;
  flex-shrink: 0;
  text-align: right;
  padding-right: 20px;
}
.navigation .mingxi .mxs .mxs_r {
  display: flex;
  justify-content: space-between;
  color: #767676;
  flex-grow: 1;
}
.navigation .H50 {
  height: 100px;
}
.navigation .foot {
  position: fixed;
  bottom: 0;
  background-color: #fff;
  width: 100%;
  padding: 10px;
  display: flex;
  justify-content: space-between;
}
.navigation .foot .foot_l {
  display: flex;
}
.navigation .foot .foot_l .foot_l_01 ._span {
  font-size: 24px;
  padding-right: 5px;
}
.navigation .foot .foot_l .foot_l_02 {
  padding-top: 8px;
  color: #767676;
}

